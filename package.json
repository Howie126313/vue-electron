    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^3.0.0",
    "electron-icon-maker": "^0.0.4",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "vue-cli-plugin-electron-builder": "^1.0.4",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
